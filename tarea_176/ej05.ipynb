    "palabra = \"palindromoomordnilap\"\n",
    "\n",
    "palindromo = True\n",
    "longitud = len(palabra)\n",
    "cont = 1\n",
    "\n",
    "while cont <= longitud//2:\n",
    "    if palabra[cont-1] != palabra[longitud-cont]:\n",
    "        palindromo = False\n",
    "    cont += 1\n",
    "\n",
    "\n",
    "print(palindromo)"
   ]
